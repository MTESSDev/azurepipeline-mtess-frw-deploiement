{
    "manifestVersion": 1,
    "id": "mtess-frw-deploiement",
    "publisher": "MTESS",
    "version": "1.0.15",
    "name": "MTESS - Déploiement de formulaires",
    "description": "Une extension simple pour déployer ses formulaires FRW.",
    "public": true,
    "categories": ["Azure Pipelines"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "branding": {
        "color": "#DAE6F0"
      },
    "icons": {
        "default": "images/icon-400.png"
    },
    "screenshots": [
        {
          "path": "images/screen1.png"
        }
      ],
    "content": {
        "details": {
            "path": "details.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/MTESSDev/azurepipeline-mtess-frw-deploiement"
    },
    "links": {
        "support": {
          "uri": "https://github.com/MTESSDev/azurepipeline-mtess-frw-deploiement/issues"
        }
    },
    "contributions": [
        {
            "id": "Deploiement",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Deploiement"
            }
        }
    ],
    "files": [{
        "path": "Deploiement"
    }]
}